{
  "name": "@github1/ajax-service",
  "private": false,
  "version": "0.4.3",
  "description": "Library for invoking HTTP requests for JSON and AMF media types",
  "main": "dist/index.js",
  "scripts": {
    "test": "build-tools test --webpack-dev-server false",
    "test:watch": "npm run test -- --watch --webpack-dev-server false",
    "test:cover": "npm run test -- --coverage",
    "build": "build-tools-build-lib && cp ./src/amf.js ./dist && cp ./src/amf.js ./dist/es5",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "author": "John Collier <john.collier4@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@github1/build-tools": "0.4.3"
  },
  "dependencies": {
    "cross-fetch": "3.1.4",
    "exponential-backoff": "2.1.1",
    "normalize-url": "4.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/github1/ajax-service"
  }
}
